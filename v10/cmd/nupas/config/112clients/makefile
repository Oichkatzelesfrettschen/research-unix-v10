LIB=/usr/lib/upas

install:
	cp route $(LIB)
	-chown bin $(LIB)/route
	-chmod 775 $(LIB)/route
	cp smtproute $(LIB)
	-chown bin $(LIB)/smtproute
	-chmod 775 $(LIB)/smtproute
	cp uucproute $(LIB)
	-chown bin $(LIB)/uucproute
	-chmod 775 $(LIB)/uucproute
	cp xunetroute $(LIB)
	-chown bin $(LIB)/xunetroute
	-chmod 775 $(LIB)/xunetroute
