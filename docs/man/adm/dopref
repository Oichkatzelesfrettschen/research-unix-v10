troff <<'!'
.so /usr/lib/tmac/tmac.s
.so /n/pipe/usr/vol2/ADM/mac
.XX "" 1 "Preface"
.nr zz \n(%#u-1u
.nr PN \n(%#u
.de PT
.lt \\n(LLu
.pc %
.nr PN \\n%u+\\n(zzu
.if \\n%-1 .if o \\*(%o
.if \\n%-1 .if e \\*(%e
.lt \\n(.lu
..
.de BT
.nr PN \\n%u+\\n(zzu
.nr PX \\n(.s
.nr PF \\n(.f
.ft 1
.ps \\n(PS
'lt \\n(LTu
.po \\n(POu
.if o \\*(%O
.if e \\*(%E
.ft \\n(PF
.ps \\n(PX
..
.nr HM \n(HMu-10p
.nr FO \n(FOu+10p
.so /usr/man/man0/preface
!
reccp /usr/man/man0/preface man0
